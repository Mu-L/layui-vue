{
  "name": "@layui/layui-vue",
  "version": "2.17.7",
  "author": "就眠儀式",
  "license": "MIT",
  "description": "a component library for Vue 3 base on layui-vue",
  "homepage": "http://www.layui-vue.com",
  "type": "module",
  "main": "dist/es/index.js",
  "unpkg": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "web-types": "dist/types/web-types.json",
  "types": "dist/types/index.d.ts",
  "style": "dist/lib/index.css",
  "vetur": {
    "tags": "dist/types/tags.json",
    "attributes": "dist/types/attributes.json"
  },
  "sideEffects": [
    "dist/lib/*.css",
    "dist/es/**/*.css",
    "dist/umd/*.css"
  ],
  "keywords": [
    "vue",
    "vue-component",
    "component-library",
    "layui-vue",
    "layui"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/es/index.js"
      }
    },
    "./lib/": "./dist/lib/",
    "./es/": "./dist/es/",
    "./types/*": [
      "./dist/types/*.d.ts"
    ]
  },
  "scripts": {
    "build": "pnpm run --filter . --parallel /^build:.*/",
    "build:all": "vite build --emptyOutDir --config ./script/build.all.ts",
    "build:es": "vite build --emptyOutDir --config ./script/build.es.ts",
    "build:umd": "vite build --emptyOutDir --config ./script/build.umd.ts",
    "build:dts": "vue-tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "test": "vitest run"
  },
  "dependencies": {
    "@babel/types": "7.21.0",
    "@ctrl/tinycolor": "^3.4.1",
    "@floating-ui/dom": "1.6.5",
    "@layui/icons-vue": "workspace:*",
    "@layui/layer-vue": "workspace:*",
    "@rollup/plugin-terser": "0.4.4",
    "@types/qrcode": "1.5.0",
    "@umijs/ssr-darkreader": "^4.9.45",
    "@vueuse/core": "8.7.3",
    "async-validator": "^4.1.1",
    "cropperjs": "^1.5.12",
    "dayjs": "^1.11.10",
    "evtd": "^0.2.3",
    "jsbarcode": "3.11.5",
    "qrcode": "1.5.0",
    "vue-i18n": "9.6.3"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "current node",
    "last 2 versions and > 2%",
    "ie > 10"
  ]
}
