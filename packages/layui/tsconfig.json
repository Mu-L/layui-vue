{
    "compilerOptions": {
      "baseUrl": ".",
      "module": "esnext",
      "target": "es2015",
      "moduleResolution": "Node",
      "strict": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "declaration": true,
      "emitDeclarationOnly": true,
      "esModuleInterop": true,
      "declarationDir": "dist/types",
