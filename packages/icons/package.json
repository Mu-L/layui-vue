{
  "name": "@layui/icons-vue",
  "version": "1.1.1",
  "description": "a component library for Vue 3 base on layui-vue",
  "homepage": "http://www.layui-vue.com",
  "module": "dist/lib/icons-vue.es.js",
  "main": "dist/lib/icons-vue.umd.js",
  "exports": {
    ".": {
      "import": "./dist/lib/icons-vue.es.js",
      "require": "./dist/lib/icons-vue.umd.js"
    },
    "./lib/": "./dist/lib/"
  },
  "types": "dist/types/index.d.ts",
  "style": "dist/lib/index.css",
  "scripts": {
    "generate": "node ./scripts/generate.ts",
    "build": "vite build --emptyOutDir && npm run build:types",
    "build:types": "vue-tsc -d"
  },
  "keywords": [
    "layui-icon",
    "icons-vue",
    "vue"
  ],
  "author": "就眠儀式",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "current node",
    "last 2 versions and > 2%",
    "ie > 10"
  ]
}
