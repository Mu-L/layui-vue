{
  "name": "@layui/json-schema-form",
  "version": "1.0.11",
  "description": "Json-schema-form is a high-level component encapsulated on the basis of @layui/layui-vue form.",
  "homepage": "http://www.layui-vue.com/zh-CN/components/jsonSchemaForm",
  "type": "module",
  "module": "dist/lib/json-schema-form.es.js",
  "main": "dist/lib/json-schema-form.umd.js",
  "license": "MIT",
  "bugs": {
    "url": "https://gitee.com/layui-vue/layui-vue/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/lib/json-schema-form.es.js",
      "require": "./dist/lib/json-schema-form.umd.js"
    },
    "./lib/": "./dist/lib/",
    "./types/*": [
      "./dist/types/*.d.ts"
    ]
  },
  "types": "dist/types/index.d.ts",
  "style": "dist/lib/index.css",
  "keywords": [
    "layui-vue",
    "layui",
    "layui/json-schema-form",
    "vue"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build --emptyOutDir && npm run build:types",
    "build:types": "rimraf types && vue-tsc -d",
    "test": "vitest"
  },
  "dependencies": {
    "@layui/component": "workspace:*"
  }
}
